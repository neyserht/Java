<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="neyser.ejemplocomboboxpendientes.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="label1"/>
    <ComboBox fx:id="comboBox1" promptText="tipo de cerveza" onAction="#onF1">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:id="choice0" fx:value="Rubia"/>
                <String fx:id="choice1" fx:value="Tostada"/>
                <String fx:id="choice2" fx:value="Negra"/>
            </FXCollections>
        </items>
    </ComboBox>

    <ComboBox fx:id="comboBox2" promptText="Marca de cerveza" >
        <items>
            <FXCollections fx:factory="observableArrayList">

            </FXCollections>
        </items>
    </ComboBox>
    <Button fx:id="button1" text="Aceptar" onAction="#onF2"/>
</VBox>
